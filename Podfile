# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'NutritionAppIOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!


  # Reactive Work
  pod 'RxSwift'
  pod 'RxCocoa'


  # for images retrieval and working with them
  pod 'Kingfisher'

  # fetching requests from urls
  pod 'Alamofire', '~> 5.2'

  # Web Socket
  pod "StompClientLib"
  pod 'SwiftStomp'
  pod 'Firebase/Messaging'


  pod 'SwiftKeychainWrapper'
  # Pods for NutritionAppIOS

  target 'NutritionAppIOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'NutritionAppIOSUITests' do
    # Pods for testing
  end

  pod 'Cosmos', '~> 23.0'


end
